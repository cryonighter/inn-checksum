{
    "name": "cryonighter/inn-checksum",
    "description": "This package validates the Russian Tax Identification Number of Legal Persons, Individual Entrepreneurs and Individual Person",
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "inn",
        "nalog",
        "tax",
        "taxpayer",
        "инн",
        "налогоплательщик",
        "идентификационный номер налогоплательщика"
    ],
    "authors": [
        {
            "name": "Andrey Reshetchenko",
            "email": "cryonighter@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-pcre": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.6"
    },
    "autoload": {
        "psr-4": {
            "Cryonighter\\InnChecksum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cryonighter\\InnChecksum\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
